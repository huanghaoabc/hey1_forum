@import "/components/common.wxss";

.line{
  background-color: #fff;
  padding: 24rpx;
  font-size: 28rpx;
  margin-left: 24rpx;
  margin-right: 24rpx;
  border-radius: 30rpx;
  margin-bottom: 14rpx;
}

.line image{
  height: 100rpx;
  width: 100rpx;
  border-radius: 100%;
}

