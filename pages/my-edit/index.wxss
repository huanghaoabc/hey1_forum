  width: 160rpx;
  border-radius: 30rpx;
  background-color: #f33535;
  color: #fff;
  line-height: 60rpx;
  text-align: center;
  font-size: 28rpx;
  margin-top: 24rpx;
}

.more image{
  height: 30rpx;
  width: 30rpx;
}

