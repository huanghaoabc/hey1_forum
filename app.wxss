/*index*/
.IndexNavIcon {width:100rpx; height:100rpx; display:block; margin: 0 auto;}
.IndexNavText {width:100%; line-height:2; font-size:24rpx; text-align:center;display:block;}
.IndexGongGaoBox {padding:10rpx 40rpx; background:#fe666d}
.IndexGongGaoIcon {width:40rpx; height:40rpx;}
.IndexItems {padding:20rpx;}
.IndexHeader {width:80rpx; height:80rpx; border-radius:40rpx;}
.IndexHeaderName {width:120rpx; display: block;overflow: hidden; white-space: nowrap;  text-overflow:ellipsis;}
.IndexHeaderTime {width:120rpx; display: block; overflow: hidden; white-space: nowrap;  text-overflow:ellipsis;}
.IndexItemTips {line-height:54rpx; font-size:24rpx; padding:0 40rpx; height:54rpx;}
.IndexItemTipsImages {width:30rpx;  padding:0 10rpx;}
.IndexNumberBox {font-size:40rpx;font-weight:bold;color:#232222}
.IndexNumberBoxInfo {font-size:24rpx; color:#232222}
.ItemRow {width:710rpx; margin: 20rpx; background:#fff; border-radius:15rpx;}
.IndexItemTitle {font-size: 30rpx; font-weight: bold;}
.IndexItemUserImg {width:50rpx; height:50rpx; margin-right:20rpx; border-radius:25rpx; border:1px solid #ccc}
/*list*/
.ListItems {border-top:1px solid #ccc}
.ItemImages {width:90%; padding:5px 10% 0 0}
.ListItemImage .IndexItemTipsImages {width:20rpx; }
.ListItemImage .IndexItemTipsImages .mintext {font-size:18rpx}

/*business*/
.businessBox {padding:20rpx; }
.businessImgBox {width:200rpx;  position:relative; margin-right:20rpx;}
.businessImgBox image {width:160rpx; height:190rpx;}
.businessImgBox .headBox {height:50rpx; line-height:50rpx; }
.businessImgBox .headBox image {width:46rpx; height:46rpx; border:2rpx solid #fff; border-radius:24rpx; display:block; float:left; margin-right:10rpx; box-shadow:  0 0 6rpx rgba(0, 0, 0, 0.8);}
.businessImgBox .headBox text {display:inline-block; height:50rpx; line-height:50rpx; font-size:24rpx; font-weight: bold; color:#333; width:140rpx;}

.businessBox .quan,.businessBox .tag {display:block; height:30rpx; line-height:30rpx; border:1px solid #fc7076; background:#fc7076; padding:0 20rpx;border-radius:15rpx; color:#fff; font-size:24rpx; display: inline-block;}

.businessBox .tag {background:#d2d2d2; border:1px solid #d2d2d2; color: #fff; margin-right:10rpx; height:30rpx; line-height: 30rpx; padding:0 10rpx}
.chushouBox {width: 100vw; height:100vh; position:relative}
/*chushou*/
.chushouIcons {width:200rpx; margin:0 auto; display: block; }
.chushouIconBox {width:50vw; }
.chushouIconsTxt {display:block; width:50vw; line-height:2; text-align:center;height:100rpx; font-size:32rpx;}
.InputBorder {border:1px solid #ccc; border-radius:3px; height:56rpx; line-height:56rpx;}
.inputTit {width:150rpx; font-size:28rpx; text-align:right; height:60rpx; line-height:60rpx;} 
.SelectItemIconBtn {height: 60rpx; display:inline-block; padding:0 30rpx; border-radius:30rpx; background:#f00; color:#fff; font-size:30rpx; line-height:60rpx;}
.defaultStyle {background:#999;}
.TitleWidth {width:140rpx; text-align:justify; display:inline-block; text-align-last:justify; }
.content{
  margin:24rpx;
  background-color: #fff;
  padding: 24rpx;
  border-radius: 15rpx;
}
.linkBox {padding:5rpx 10rpx; border:1px solid #eee; margin:0 10rpx 0 0; border-radius:10rpx; line-height: 1.5; width:420rpx;}
.copyButton {line-height:1.5; padding:12rpx 20rpx; border-radius:10rpx; color:#fff; background:#f33535;}
.FixButton {width:100%; height: 100rpx; position:fixed; left:0; bottom:0; z-index:9999}
.FixButton_fabiaoBtn {width:100%; height:100px; background:#f33535; color:#fff; text-align:center; line-height:100rpx; font-size:32rpx;}
